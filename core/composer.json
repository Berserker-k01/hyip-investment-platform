{
  "name": "expertoil/hyip-max",
  "description": "HYIP MAX platform",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-pdo_pgsql": "*",
    "laravel/framework": "^8.83",
    "vlucas/phpdotenv": "^5.5"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Http/Helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php artisan package:discover --ansi || true"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
